<% if current_user != target.user %>
    <% if (target_name == "post" && LikePost.where("user_id = ? AND post_id = ?", current_user.id, target.id).exists?) || (target_name == "comment" && LikeComment.where("user_id = ? AND comment_id = ?", current_user.id, target.id).exists?)  %> 
        You've liked this <%= target_name %>
    <% else  %>
        <%= render 'shared/like_something', what: target_name, like_id: target.id %>
    <% end %>
<% end %>